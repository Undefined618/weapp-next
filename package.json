    "wexin",
    "applet小程序",
    "wx",
    "微信小程序",
    "weapp"
  ],
  "author": "xixilive@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xixilive/weapp/issues"
  },
  "homepage": "https://github.com/xixilive/weapp#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "core-js": "^2.4.1",
    "eslint": "^3.12.2",
    "jest-cli": "^18.0.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "jest": {
    "automock": false,
    "bail": true,
